<h2>スケジュール詳細</h2>

<table>
  <tr>
    <th>タイトル</th>
    <td><%= @schedule.title %></td>
  </tr>
  <tr>
    <th>開始日時</th>
    <td><%= @schedule.start %></td>
  </tr>
  <tr>
    <th>終了日時</th>
    <td><%= @schedule.end %></td>
  </tr>
  <tr>
    <th>終日</th>
    <td><%= @schedule.all_day %></td>
  </tr>
  <tr>
    <th>更新日</th>
    <td><%= @schedule.updated_at %></td>
  </tr>
  <tr>
    <th>メモ</th>
    <td><%= @schedule.memo %></td>
  </tr>
</table>

<div>
  <ul>
    <li><%= link_to "スケジュール一覧に戻る", :schedules %></li>
    <!--<li></li>-->
    <!--<li></li>-->
  </ul>
</div>